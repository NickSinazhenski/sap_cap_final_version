{
  "_version": "1.76.0",
  "sap.app": {
    "id": "app.purchase.purchaseapp.project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.19.6",
      "toolsId": "8f5903a9-db2b-4dac-841f-9b13106c824a"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/purchase/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "app.purchase.purchaseapp.project1.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "PurchaseRequestsList",
          "target": "PurchaseRequestsList"
        },
        {
          "pattern": "PurchaseRequests({key}):?query:",
          "name": "PurchaseRequestsObjectPage",
          "target": "PurchaseRequestsObjectPage"
        },
        {
          "pattern": "PurchaseRequests({key})/rates({key2}):?query:",
          "name": "PurchaseRequestCurrencyObjectPage",
          "target": "PurchaseRequestCurrencyObjectPage"
        }
      ],
      "targets": {
        "PurchaseRequestsList": {
          "type": "Component",
          "id": "PurchaseRequestsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/PurchaseRequests",
              "variantManagement": "Page",
              "navigation": {
                "PurchaseRequests": {
                  "detail": {
                    "route": "PurchaseRequestsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              },
              "defaultTemplateAnnotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#tableView"
            }
          }
        },
        "PurchaseRequestsObjectPage": {
          "type": "Component",
          "id": "PurchaseRequestsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/PurchaseRequests",
              "navigation": {
                "rates": {
                  "detail": {
                    "route": "PurchaseRequestCurrencyObjectPage"
                  }
                }
              }
            }
          }
        },
        "PurchaseRequestCurrencyObjectPage": {
          "type": "Component",
          "id": "PurchaseRequestCurrencyObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/PurchaseRequests/rates"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
